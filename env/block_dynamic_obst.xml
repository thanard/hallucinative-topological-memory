<mujoco>
    <compiler angle="radian" coordinate="global" inertiafromgeom="true" meshdir="mesh" texturedir="textures" />
    <option gravity="0 0 -9.81" integrator="Euler" iterations="20" timestep=".01" />

    <default>
        <joint armature="0.04" damping="1" limited="true" />
        <geom conaffinity="0" condim="1" contype="0" density="10" friction=".8 .1 .1" margin="0.002" />
    </default>

    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1" directional="true" exponent="1" pos="0 0 0.0" specular=".1 .1 .1"/>

        <camera name='targeting' mode='fixed' pos='0. 0. 5.'/>


        <geom rgba="0.1 0.1 .1 1." conaffinity="1" density='0.' contype="1" condim="1" name="table" pos="0 0 0" size=".8 .8 0.1" type="plane" />

        <body name="obj1">
            <joint axis="1 0 0" damping="1.5" name="obj1_slidex" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <joint axis="0 1 0" damping="1.5" name="obj1_slidey" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <joint axis="0 0 1" damping="1.5" name="obj1_slidez" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <geom conaffinity="1" contype="1" pos="0 0 0." size="0.12 0.12 0.12" rgba="0.0  1.0 0.0 1" type="box" />
        </body>


        <body name="obs1">
            <joint axis="1 0 0" damping="1.5" name="obs1_slidex" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <joint axis="0 1 0" damping="1.5" name="obs1_slidey" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <joint axis="0 0 1" damping="1.5" name="obs1_slidez" pos="0 0 0" range="-10.3213 10.3" type="slide" />
            <geom conaffinity="1" contype="1" pos="0. 0. 0." size="0.06 0.3 0.12" rgba="1.0  0.0 0.0 1" type="box" />
        </body>


    </worldbody>

    <actuator>

        <motor joint="obj1_slidex" gear="18"/>
        <motor joint="obj1_slidey" gear="18"/>
        <motor joint="obj1_slidez" gear="18"/>
        <motor joint="obs1_slidex" gear="18"/>
        <motor joint="obs1_slidey" gear="18"/>
        <motor joint="obs1_slidez" gear="18"/>

    </actuator>

</mujoco>
